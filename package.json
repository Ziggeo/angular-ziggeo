{
  "name": "angular-ziggeo",
  "version": "4.0.0",
  "description": "Ziggeo recorder and player, Angular 9 > integration ",
  "main": "./build/index.umd",
  "module": "./dist/angular-ziggeo/modules/ziggeo.module",
  "typings": "./dist/angular-ziggeo/types.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ziggeo/angular-ziggeo.git"
  },
  "bugs": {
    "url": "https://github.com/Ziggeo/angular-ziggeo/issues"
  },
  "keywords": [
    "Ziggeo",
    "Angular-8",
    "Ziggeo Player",
    "Ziggeo Recorder",
    "Player",
    "recorder"
  ],
  "author": {
    "name": "Ziggeo Inc",
    "ulr": "https://ziggeo.com"
  },
  "homepage": "https://ziggeo.com",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=12.10",
    "yarn": ">=1.15.2 <2.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "rm -rf dist && ng-packagr -p projects/angular-ziggeo/package.json",
    "pack": "cd dist/angular-ziggeo && npm pack",
    "publish": "cd dist/angular-ziggeo && npm publish",
    "reinstall": "rm -rf node_modules/angular-ziggeo yarn.lock && yarn install",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ci": "yarn check-ts && yarn build --progress false",
    "changelog": "conventional-changelog --preset angular --infile CHANGELOG.md --same-file --release-count 1",
    "tslint": "tslint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2",
    "ziggeo-client-sdk": "~2.39.16",
    "angular-ziggeo": "file:dist/angular-ziggeo"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "@angular-devkit/build-ng-packagr": "~0.900.0",
    "conventional-changelog-cli": "^2.0.12",
    "cross-spawn": "^7.0.1",
    "ng-packagr": "~9.0.0"
  }
}
